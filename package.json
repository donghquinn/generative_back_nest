{
  "name": "chpt",
  "version": "1.5.0",
  "main": "app.ts",
  "license": "LGPL-3.0-only",
  "type": "module",
  "author": "donghquinn",
  "packageManager": "yarn@4.1.0",
  "scripts": {
    "format": "prettier --write \"**/*.{ts,json,md}\" \"!package*.json\" \"!tsconfig.json\" \"!.eslintrc.json\"",
    "dev": "NODE_ENV=development node -r ts-node/register --loader ts-paths-esm-loader --experimental-specifier-resolution=node ./src/app.ts",
    "build": "NODE_ENV=production tsc --project tsconfig.json --diagnostics -p tsconfig.json",
    "start": "NODE_ENV=production node -r  ts-node/register --loader ts-paths-esm-loader --experimental-specifier-resolution=node ./src/app.ts",
    "dev:chat": "nodemon --exec node -r  ts-node/register --loader ts-paths-esm-loader --experimental-specifier-resolution=node ./src/libraries/providers/chat/test.lib.ts",
    "migrate": "prisma migrate dev --schema=./src/assets/schema.prisma --name chatgpt",
    "schema": "prisma generate --schema=./src/assets/schema.prisma",
    "db:update": "prisma db push --schema=./src/assets/schema.prisma",
    "test": "node --experimental-vm-modules node_modules/.bin/jest"
  },
  "dependencies": {
    "@gradio/client": "^0.12.1",
    "@nestjs/common": "^10.3.3",
    "@nestjs/core": "^10.3.3",
    "@nestjs/platform-express": "^10.3.3",
    "@prisma/client": "^5.10.2",
    "axios": "^1.4.0",
    "bignumber.js": "^9.1.1",
    "crypto": "^1.0.1",
    "crypto-js": "^4.2.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.0",
    "minio": "^7.1.2",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.9.11",
    "openai": "^4.20.1",
    "prisma": "^5.10.2",
    "redis": "^4.6.13",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1",
    "uuid": "^9.0.1",
    "winston": "^3.9.0",
    "winston-daily-rotate-file": "^4.7.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bignumber.js": "^5.0.0",
    "@types/crypto-js": "^4",
    "@types/express": "^4.17.20",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.6",
    "@types/nodemailer": "^6.4.14",
    "@types/redis": "^4.0.11",
    "@types/uuid": "^9.0.8",
    "dotenv": "^16.4.5",
    "eslint": "^8.54.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eventsource": "^2.0.2",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "ts-paths-esm-loader": "^1.4.3",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "prisma": {
    "schema": "./src/assets/schema.prisma"
  }
}
